{
  "name": "@hirosystems/docs",
  "version": "0.1.0",
  "private": true,
  "prettier": "@stacks/prettier-config",
  "scripts": {
    "build": "yarn update:cli-ref && yarn pull-openapi-definitions && docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start",
    "docusaurus": "docusaurus",
    "postinstall": "patch-package",
    "serve": "docusaurus serve",
    "spellcheck": "vale --config='.vale.ini' docs/**",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc",
    "update:cli": "yarn upgrade @stacks/cli@latest --dev",
    "update:cli-ref": "stx docs > src/_data/cli-reference.json && node ./update-cli-ref.js ./src/_data/cli-reference.json -o ./docs/references/",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "pull-openapi-definitions": "rimraf ./openapi && node -r ts-node/register ./util/pull-openapi-definitions.ts"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.3",
    "@docusaurus/module-type-aliases": "^2.4.3",
    "@docusaurus/preset-classic": "^2.4.3",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^6.3.1",
    "clsx": "^1.1.1",
    "docusaurus-plugin-openapi": "^0.6.4",
    "docusaurus-plugin-remote-content": "^3.1.0",
    "docusaurus-plugin-segment": "^1.0.3",
    "docusaurus-preset-openapi": "^0.6.4",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redocusaurus": "^1.6.2",
    "url-loader": "^4.1.1",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@stacks/cli": "^6.0.2",
    "@stacks/prettier-config": "^0.0.10",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.2",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "url": "^0.11.1"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
