{
  "name": "@hirosystems/docs",
  "version": "0.1.0",
  "private": true,
  "prettier": "@stacks/prettier-config",
  "scripts": {
    "build": "yarn update:cli-ref && yarn pull-openapi-definitions && docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start",
    "docusaurus": "docusaurus",
    "postinstall": "patch-package",
    "serve": "docusaurus serve",
    "spellcheck": "vale --config='.vale.ini' docs/**",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc",
    "update:cli": "yarn upgrade @stacks/cli@latest --dev",
    "update:cli-ref": "stx docs > src/_data/cli-reference.json && node ./update-cli-ref.js ./src/_data/cli-reference.json -o ./docs/references/",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "pull-openapi-definitions": "rimraf ./openapi && node -r ts-node/register ./util/pull-openapi-definitions.ts"
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/module-type-aliases": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@egoist/tailwindcss-icons": "^1.3.3",
    "@iconify-json/bi": "^1.1.20",
    "@iconify-json/pixelarticons": "^1.1.7",
    "@iconify-json/radix-icons": "^1.1.11",
    "@mdx-js/react": "^3.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@svgr/webpack": "^6.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "docusaurus-plugin-openapi": "^0.7.0",
    "docusaurus-plugin-remote-content": "^3.1.0",
    "docusaurus-plugin-segment": "^1.0.3",
    "docusaurus-preset-openapi": "^0.7.0",
    "file-loader": "^6.2.0",
    "lucide-react": "^0.292.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redocusaurus": "^1.6.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "url-loader": "^4.1.1",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@docusaurus/types": "^3.1.0",
    "@stacks/cli": "^6.11.1",
    "@stacks/prettier-config": "^0.0.10",
    "@tsconfig/docusaurus": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "patch-package": "^7.0.0",
    "postcss": "^8.4.31",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "url": "^0.11.1"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
